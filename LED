#define BUTTON_PIN 15
#define LED_HIJAU 19

void setup() {
  pinMode(BUTTON_PIN, INPUT_PULLUP); // Butang aktif LOW
  pinMode(LED_HIJAU, OUTPUT);
}

void loop() {
  // Jika push button ditekan
  if (digitalRead(BUTTON_PIN) == HIGH) {
    digitalWrite(LED_HIJAU, HIGH);  // Lampu hijau hidup
  } else {
    digitalWrite(LED_HIJAU, LOW);   // Lampu hijau padam
  }
}
